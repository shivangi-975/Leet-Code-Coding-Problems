class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        n=len(digits)-1;
        while(n>=0):
            if(digits[n]==9):
                digits[n]=0
            else:
                digits[n]=digits[n]+1;
                return digits
            n=n-1
        return [1]+digits
        
            
        
        
