class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        n=len(nums);
        ptr=0;
        for i in range(0,n):
             if(nums[i]!=val):
                    
                    
                    nums[ptr]=nums[i]
                    
                    ptr=ptr+1
        return ptr
            
