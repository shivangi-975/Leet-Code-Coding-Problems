class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        i=0
        if(len(nums)==0):
            return 0  
        for j in range(i+1,len(nums)):
            if(nums[i]!=nums[j]):
                  i=i+1
                  nums[i]=nums[j]
        return i+1 
        
    
        
