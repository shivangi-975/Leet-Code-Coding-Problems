class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        int n=word1.size();
        int m=word2.size();
        string s;
        string p;
        for(int i=0;i<n;i++)
        {
            s=s+word1[i];
        }
        for(int i=0;i<m;i++)
        {
            p=p+word2[i];
        }
        if(s==p)
        {
            return true;
        }
        return false;
    }
};
