class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        int p;
        int count=0;
        map<int,int>m;
        for(int i=0;i<n;i++)
        {
          m[nums[i]]++;
        }
        for(auto x:m)
        {
           if(x.second>n/2)
           {
               p=x.first;
           }
       
    }
         return p;
    }
};
